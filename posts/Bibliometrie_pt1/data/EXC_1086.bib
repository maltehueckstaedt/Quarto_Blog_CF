@inproceedings{ WOS:000406120700031,
Author = {Scheibler, Karsten and Neubauer, Felix and Mahdi, Ahmed and Fraenzle,
   Martin and Teige, Tino and Bienmueller, Tom and Fehrer, Detlef and
   Becker, Bernd},
