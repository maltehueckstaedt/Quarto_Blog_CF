@article{ WOS:000457329700018,
Author = {Mueller, Angelina and Wapler, Matthias C. and Wallrabe, Ulrike},
Title = {A quick and accurate method to determine the Poisson's ratio and the
   coefficient of thermal expansion of PDMS},
Journal = {SOFT MATTER},
Year = {2019},
Volume = {15},
Number = {4},
Pages = {779-784},
Month = {JAN 28},
Abstract = {We present a new and accurate method to determine the Poisson's ratio of
   PDMS, using thermal expansion and an optical surface profilometer. The
   Poisson's ratio of Sylgard 184 was found to be nu = 0.4950 + 0.0010 and
   for Sylgard 182, nu = 0.4974 + 0.0006. Furthermore, we found that for
   both PDMS types, the coefficient of thermal expansion depends
   approximately linearly on the curing temperature. This method can be
   used for almost any kind of soft polymer that can be cured from a liquid
   at elevated temperatures.},
