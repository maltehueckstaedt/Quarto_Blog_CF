@article{ WOS:000384715800117,
Author = {Mueller, Angelina and Wapler, Matthias C. and Schwarz, Ulrich T. and
   Reisacher, Markus and Holc, Katarzyna and Ambacher, Oliver and Wallrabe,
   Ulrike},
Title = {Quasi-Bessel beams from asymmetric and astigmatic illumination sources},
Journal = {OPTICS EXPRESS},
Year = {2016},
Volume = {24},
Number = {15},
Pages = {17433-17452},
Month = {JUL 25},
Abstract = {We study the spatial intensity distribution and the self-reconstruction
   of quasi-Bessel beams produced from refractive axicon lenses with edge
   emitting laser diodes as asymmetric and astigmatic illumination sources.
   Comparing these to a symmetric mono-mode fiber source, we find that the
   asymmetry results in a transition of a quasi-Bessel beam into a bow-tie
   shaped pattern and eventually to a line shaped profile at a larger
   distance along the optical axis. Furthermore, we analytically estimate
   and discuss the effects of astigmatism, substrate modes and non-perfect
   axicons. We find a good agreement between experiment, simulation and
   analytic considerations. Results include the derivation of a maximal
   axicon angle related to astigmatism of the illuminating beam, impact of
   laser diode beam profile imperfections like substrate modes and a
   longitudinal oscillation of the core intensity and radius caused by a
   rounded axicon tip. (C) 2016 Optical Society of America},
