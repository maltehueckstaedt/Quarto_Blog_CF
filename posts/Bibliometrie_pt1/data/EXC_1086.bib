@article{ WOS:000383346100020,
Author = {Kumar, Sreedhar S. and Wuelfing, Jan and Okujeni, Samora and Boedecker,
   Joschka and Riedmiller, Martin and Egert, Ulrich},
Title = {Autonomous Optimization of Targeted Stimulation of Neuronal Networks},
Journal = {PLOS COMPUTATIONAL BIOLOGY},
