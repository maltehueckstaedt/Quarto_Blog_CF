@article{carrollInterpointDistanceComparisons1986,
  title = {Interpoint {{Distance Comparisons}} in {{Correspondence Analysis}}},
  author = {Carroll, J. Douglas and Green, Paul E. and Schaffer, Catherine M.},
  year = {1986},
  journal = {Journal of Marketing Research},
  volume = {23},
  number = {3},
  pages = {271--280},
  publisher = {{American Marketing Association}},
  issn = {0022-2437},
  doi = {10.2307/3151485},
  abstract = {Correspondence analysis is a metric technique for finding a spatial representation of data that has particular applicability to the analysis of cross tabulations (or contingency tables). The authors focus on some of the geometric underpinnings of the procedure and the kinds of statements that can be made about interobject similarity based on a distance model. In particular, a rationale is developed, along with a scaling of coordinates, that enables one to compare both between-set and within-set squared distances. This scaling differs from the conventional scaling currently used in correspondence analysis, where only one set of distances usually are compared.}
}

